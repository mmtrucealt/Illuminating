{
  "version": 1,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/gt/(.*)",
      "headers": {
        "Service-Worker-Allowed": "/"
      },
      "dest": "/public/uv.sw.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
